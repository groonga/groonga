msgstr ""

msgid "Output nothing into the log."
msgstr "ログに何も出力しません。"

msgid "``time``"
msgstr ""

msgid "Output a timestamp into the log."
msgstr "ログにタイムスタンプを出力します。"

msgid "``message``"
msgstr ""

msgid "Output log messages into the log."
msgstr "ログにメッセージを出力します。"

msgid "``location``"
msgstr ""

