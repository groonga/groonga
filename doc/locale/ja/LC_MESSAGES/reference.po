msgstr "ダブル配列トライ"

msgid "Hash table"
msgstr "ハッシュテーブル"

msgid "Patricia trie"
msgstr "パトリシアトライ"

msgid "ID support"
msgstr "IDサポート"

msgid "Key support"
msgstr "キーサポート"

msgid "Value support"
msgstr "バリューサポート"

